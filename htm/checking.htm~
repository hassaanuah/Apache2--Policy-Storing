<FORM NAME="joe">
<INPUT TYPE="hidden" NAME="burns">
</FORM>




<SCRIPT LANGUAGE="javascript">

var locate = window.location
document.joe.burns.value = locate

var text = document.joe.burns.value

function delineate(str)
{
theleft = str.indexOf("=") + 1;
theright = str.lastIndexOf("&");
return(str.substring(theleft, theright));
}
document.write("First Name is " +delineate(text));
document.write("<br> <input type='hidden' id='fqdn' value='" + delineate(text) + "'></input>" );
function delineate2(str)
{
point = str.lastIndexOf("=");
return(str.substring(point+1,str.length));
}
document.write("<br> Last Name is " +delineate2(text));
document.write("<br> <input type='hidden' id='msisdn' value='" + delineate2(text) + "'></input>" );
</SCRIPT> 
